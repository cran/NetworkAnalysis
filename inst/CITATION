citHeader("To cite package 'NetworkAnalysis' in publications please use:")

desc <- packageDescription("NetworkAnalysis")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", desc$Date)
vers <- paste("R package version", desc$Version)

citEntry(entry="Manual",
title = "NetworkAnalysis: ",
author = personList(as.person("Cedric E Ginestet")),
year = year,
note = vers,
textVersion =
paste("Ginestet C.E. (",year,"). 
NetworkAnalysis: Statistical inference on populations of 
weighted and unweighted networks.",vers,".", sep=""))